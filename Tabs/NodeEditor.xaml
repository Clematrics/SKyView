<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SkyView.Tabs"
             xmlns:SkyView="clr-namespace:SkyView" x:Class="SkyView.Tabs.NodeEditor"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" SizeChanged="NodeEditor_SizeChanged">
    <Canvas x:Name="graphCanvas" ClipToBounds="True">
        <Canvas x:Name="container" Width="4096" Height="4096">
            <Border x:Name="background" Width="4096" Height="4096" Background="{StaticResource NodeEditorBackgroundTiled}" MouseMove="container_MouseMove" MouseLeftButtonDown="container_MouseDown" MouseLeftButtonUp="container_MouseUp"/>
        </Canvas>
        <Canvas x:Name="linkContainer" Width="4096" Height="4096">
            
        </Canvas>
    </Canvas>
</UserControl>
